},{"filePath":"61","messages":"62","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"34"},{"filePath":"63","messages":"64","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"34"},{"filePath":"65","messages":"66","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"34"},"/Users/i354110/Documents/ReactApps/disbook/client/src/index.js",[],["67","68"],"/Users/i354110/Documents/ReactApps/disbook/client/src/App.js",[],"/Users/i354110/Documents/ReactApps/disbook/client/src/reportWebVitals.js",[],"/Users/i354110/Documents/ReactApps/disbook/client/src/redux/store.js",[],"/Users/i354110/Documents/ReactApps/disbook/client/src/redux/reducer.js",[],"/Users/i354110/Documents/ReactApps/disbook/client/src/components/books/AddBook.js",["69"],"import React, { useState, useEffect } from 'react';\nimport { FaArrowLeft, FaBook, FaCalculator, FaStickyNote, FaUser } from 'react-icons/fa';\nimport { Link } from 'react-router-dom';\nimport { useDispatch } from 'react-redux';\nimport { useHistory, useParams } from 'react-router-dom';\nimport { addBook, getBook, editBook } from '../../api/booksApi';\n\nconst AddBook = () => {\n    let history = useHistory();\n    const {id} = useParams();\n    const [book, setBook] = useState({\n        name: '',\n        description: '',\n        author: '',\n        count: ''\n    });\n    const dispatch = useDispatch();\n    \n    const loadBook = async () => {\n        const result = await getBook(id);\n        setBook(result);\n    };\n    useEffect(() => {\n        if (id) {\n            loadBook();\n        }\n    }, []);\n\n    const handleSubmit = e => {\n        id ? dispatch(editBook(book)) : dispatch(addBook(book));\n        history.push('/');\n        e.preventDefault();\n    }\n    \n    const handleChange = e => {\n        const target = e.target;\n        setBook({\n            ...book,\n            [target.name]: target.value\n        });\n    }\n    return (\n        <div className=\"form-container\">\n            <form onSubmit={handleSubmit} className=\"add-form\">\n                <Link to={'/'}><FaArrowLeft/></Link>\n                <h2>Add/Edit Books</h2>\n                <div className=\"input-container\">\n                    <i className=\"icon\"><FaBook/></i>\n                    <input className=\"input-field\" type=\"text\" placeholder=\"Book Name\" \n                        value={book.name} onChange={handleChange} name=\"name\"/>\n                </div>\n                <div className=\"input-container\">\n                    <i className=\"icon\"><FaStickyNote/></i>\n                    <textarea rows=\"4\" cols=\"10\" className=\"input-field\" type=\"text\" placeholder=\"Description\"\n                        value={book.description} onChange={handleChange} name=\"description\"/>\n                </div>\n                <div className=\"input-container\">\n                    <i className=\"icon\"><FaUser/></i>\n                    <input className=\"input-field\" type=\"text\" placeholder=\"Author\"\n                        value={book.author} onChange={handleChange}  name=\"author\"/>\n                </div>\n                <div className=\"input-container\">\n                    <i className=\"icon\"><FaCalculator/></i>\n                    <input className=\"input-field\" type=\"number\" placeholder=\"Count\" \n                        value={book.count} onChange={handleChange} name=\"count\"/>\n                </div>\n                <button type=\"submit\" className=\"btn\">Save</button>\n            </form>\n        </div>\n    );\n}\nexport default AddBook;\n","/Users/i354110/Documents/ReactApps/disbook/client/src/components/books/BookList.js",[],"/Users/i354110/Documents/ReactApps/disbook/client/src/components/home/Home.js",["70"],"import React, { useEffect, useRef, useState } from 'react';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { Link } from 'react-router-dom';\nimport { getBooks } from '../../api/booksApi';\nimport BookList from '../books/BookList';\nimport Filter from '../filters/Filter';\n\nconst debounce = (callback, wait, immediate = false) =>{\n    let timeout = null \n    \n    return function() {\n      const callNow = immediate && !timeout\n      const next = () => callback.apply(this, arguments)\n      \n      clearTimeout(timeout)\n      timeout = setTimeout(next, wait)\n  \n      if (callNow) {\n        next()\n      }\n    }\n};\n\nconst Home = () => {\n    const dispatch = useDispatch();\n    const bookStore = useSelector(state => state.books);\n    const [books, setBooks] = useState(bookStore);\n    const [isLoading, setIsLoading] = useState(false);\n    const inputRef = useRef();\n\n    useEffect(() => {\n        setIsLoading(true);\n        dispatch(getBooks());\n        inputRef.current = debounce(onSearchText, 500);\n    }, [])\n\n    useEffect(() => {\n        if (bookStore.length > 0) {\n            setBooks(bookStore);\n            setIsLoading(false);\n        }\n      }, [bookStore]);\n\n    const onSearchText = (text, bookStore) => {\n        let filtered;\n        if (text) {\n            filtered = bookStore.filter((book) =>\n                book.name.toLowerCase().includes(text.toLowerCase())\n            );\n        } else {\n          filtered = bookStore;\n        }\n        setBooks(filtered);\n      }\n\n    const handleSearch = e => {\n        const inputValue = e.target.value.trim();\n        const callImmediate = e.type === 'submit';\n        inputRef.current(inputValue, bookStore, callImmediate);\n        e.preventDefault();\n    }\n    return (\n        <div>\n            <Filter handleSearch={handleSearch}></Filter>\n            <Link to={'/books/add'}>Add Book</Link>\n            <BookList books={books} isLoading={isLoading}></BookList>\n        </div>\n    );\n}\n\nexport default Home;\n","/Users/i354110/Documents/ReactApps/disbook/client/src/components/header/Header.js",[],"/Users/i354110/Documents/ReactApps/disbook/client/src/components/books/BookReducer.js",[],"/Users/i354110/Documents/ReactApps/disbook/client/src/redux/actionTypes.js",[],"/Users/i354110/Documents/ReactApps/disbook/client/src/components/books/BookListItem.js",["71"],"import React from 'react';\nimport { useDispatch } from 'react-redux';\nimport { Link } from 'react-router-dom';\nimport { deleteBook } from '../../api/booksApi';\n\nconst BookListItem = ({ book }) => {\n    const dispatch = useDispatch();\n    const handleClick = e => {\n        \n        dispatch(deleteBook(book.id));\n        e.preventDefault();\n        // dispatch({type: \"DELETE_BOOK\", id: book.id});\n    }\n    return (\n        <div className=\"book-list-item\">\n            <h3>{book.name ? book.name : '-----'}</h3>\n            <p>{book.description}</p>\n            <p>Author: {book.author}</p>\n            <p>Count: {book.count}</p>\n            <Link to={`/books/edit/${book.id}`}>Edit</Link>\n            <a href=\"#\" onClick={handleClick}>Delete</a>\n        </div>\n    );\n}\nexport default BookListItem;",["72","73"],"/Users/i354110/Documents/ReactApps/disbook/client/src/api/booksApi.js",[],"/Users/i354110/Documents/ReactApps/disbook/client/src/components/filters/Filter.js",[],"/Users/i354110/Documents/ReactApps/disbook/client/src/api/client.js",[],{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","severity":1,"message":"79","line":27,"column":8,"nodeType":"80","endLine":27,"endColumn":10,"suggestions":"81"},{"ruleId":"78","severity":1,"message":"82","line":35,"column":8,"nodeType":"80","endLine":35,"endColumn":10,"suggestions":"83"},{"ruleId":"84","severity":1,"message":"85","line":21,"column":13,"nodeType":"86","endLine":21,"endColumn":47},{"ruleId":"74","replacedBy":"87"},{"ruleId":"76","replacedBy":"88"},"no-native-reassign",["89"],"no-negated-in-lhs",["90"],"react-hooks/exhaustive-deps","React Hook useE