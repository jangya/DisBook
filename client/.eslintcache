   name: '',\n        description: '',\n        author: '',\n        count: ''\n    });\n    const dispatch = useDispatch();\n    const loadBook = async () => {\n        const result = await getBook(id);\n        setBook(result);\n    };\n    useEffect(() => {\n        if (id) {\n            loadBook();\n        }\n    }, []);\n\n    const handleSubmit = e => {\n        if (id) {\n            dispatch(editBook(book));\n        }\n        else {\n            dispatch(addBook(book));\n        }\n        \n        history.push('/');\n        e.preventDefault();\n    }\n    \n    const handleChange = e => {\n        const target = e.target;\n        setBook({\n            ...book,\n            [target.name]: target.value\n        });\n        e.preventDefault();\n    }\n    return (\n        <div>\n            <form onSubmit={handleSubmit} className=\"add-form\">\n                <label>Book Name</label>\n                <input type=\"text\" name=\"name\" value={book.name} onChange={handleChange}/>\n                <label>Description</label>\n                <textarea rows=\"4\" cols=\"10\" value={book.description} name=\"description\" onChange={handleChange}>\n                </textarea>\n                <label>Author</label>\n                <input type=\"text\" name=\"author\" value={book.author} onChange={handleChange}/>\n                <label>Count</label>\n                <input type=\"number\" name=\"count\" value={book.count} onChange={handleChange}/>\n                <button type=\"submit\">Save</button>\n            </form>\n        </div>\n    );\n}\nexport default AddBook;\n","/Users/i354110/Documents/ReactApps/disbook/client/src/components/books/BookList.js",[],"/Users/i354110/Documents/ReactApps/disbook/client/src/components/home/Home.js",["71"],"import React, { useEffect, useState } from 'react';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { Link } from 'react-router-dom';\nimport { getBooks } from '../../api/booksApi';\nimport BookList from '../books/BookList';\nimport Filter from '../filters/Filter';\n\nconst Home = () => {\n    const dispatch = useDispatch();\n    const books = useSelector(state => state.books);\n    useEffect(() => {\n        // if (!books.length)\n        dispatch(getBooks());\n    }, [books, dispatch])\n\n    return (\n        <div>\n            <Filter></Filter>\n            <Link to={'/books/add'}>Add Book</Link>\n            <BookList books={books}></BookList>\n        </div>\n    );\n}\n\nexport default Home;\n","/Users/i354110/Documents/ReactApps/disbook/client/src/components/header/Header.js",[],"/Users/i354110/Documents/ReactApps/disbook/client/src/components/books/BookReducer.js",[],"/Users/i354110/Documents/ReactApps/disbook/client/src/redux/actionTypes.js",[],"/Users/i354110/Documents/ReactApps/disbook/client/src/components/books/BookListItem.js",["72"],"import React from 'react';\nimport { useDispatch } from 'react-redux';\nimport { Link } from 'react-router-dom';\nimport { deleteBook } from '../../api/booksApi';\n\nconst BookListItem = ({ book }) => {\n    const dispatch = useDispatch();\n    const handleClick = e => {\n        e.preventDefault();\n        dispatch(deleteBook(book.id));\n        // dispatch({type: \"DELETE_BOOK\", id: book.id});\n    }\n    return (\n        <div className=\"book-list-item\">\n            <h3>{book.name ? book.name : '-----'}</h3>\n            <p>Author: {book.author}</p>\n            <p>Count: {book.count}</p>\n            <Link to={`/books/edit/${book.id}`}>Edit</Link>\n            <a href=\"#\" onClick={handleClick}>Delete</a>\n        </div>\n    );\n}\nexport default BookListItem;","/Users/i354110/Documents/ReactApps/disbook/client/src/api/booksApi.js",[],"/Users/i354110/Documents/ReactApps/disbook/client/src/components/filters/Filter.js",[],"/Users/i354110/Documents/ReactApps/disbook/client/src/api/client.js",[],{"ruleId":"73","replacedBy":"74"},{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","severity":1,"message":"78"