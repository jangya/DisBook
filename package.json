{
    "name": "disbook-express",
    "version": "1.0.0",
    "scripts": {
      "client": "cd client && npm start",
      "server": "nodemon --ignore 'server/data/*.json' server/server.js",
      "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
    },
    "dependencies": {
      "body-parser": "^1.18.3",
      "express": "^4.16.4"
    },
    "devDependencies": {
      "concurrently": "^4.0.1"
    }
  }